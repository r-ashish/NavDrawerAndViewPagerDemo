            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".findBooks"
            android:label="@string/title_activity_find_books" >
        </activity>
    </application>

</manifest>
